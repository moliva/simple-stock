{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/sort-comp": 0,
    "comma-dangle": [2, "never"],
    "no-shadow": 0,
    "brace-style": [2, "stroustrup"]
  },
  "plugins": [
    "react"
  ]
}
